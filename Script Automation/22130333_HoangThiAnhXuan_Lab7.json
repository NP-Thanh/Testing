  {
    "id": "PRD_MGT_Display_1",
    "description": "Đăng nhập và chuyển đến trang Quản lý Sản phẩm (PM)",
    "module": "PRD_MGT",
    "steps": [
      {
        "action": "goto",
        "target": "http://localhost:8080/web_th_war/home"
      },
      {
        "action": "click",
        "target": "xpath=//a[contains(@class,'bt_user')]"
      },
      {
        "action": "fill",
        "target": "#email",
        "value": "xhoang345@gmail.com"
      },
      {
        "action": "press",
        "target": "#email",
        "key": "Tab"
      },
      {
        "action": "fill",
        "target": "#password",
        "value": "123456"
      },
      {
        "action": "press",
        "target": "#password",
        "key": "Enter"
      },
      {
        "action": "goto",
        "target": "http://localhost:8080/web_th_war/ProductManagement"
      }
    ]
  },
